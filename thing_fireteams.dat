<document signature="Hero Lab Data">
  <!-- Fireteam Thing -->
<thing
  id="Fireteam"
  name="Fireteam member"
  compset="FTMember"
  isunique="no"
  description="This is a fireteam entry.">
  
  <fieldval field="usrCandid1" value=" "/>
  <fieldval field="usrCandid2" value=" "/>
  
  <eval index="1" phase="PreTraits" priority="4900">
