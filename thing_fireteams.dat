    debug field[usrChosen1].chosen.field[name].text & ", " & field[usrChosen2].chosen.field[name].text
    
    if (field[usrChosen1].chosen.field[name].text = 0) then
      debug "It's physical"
      endif
    
    ]]>
    </eval>
  
  <child entity="ftDetails"/>
  </thing>

